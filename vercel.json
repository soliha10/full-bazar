{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/src/server.js"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
